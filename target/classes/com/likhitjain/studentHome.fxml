<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.likhitjain.StudentHome">
    <MenuBar>
        <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" onAction="#onLogout" text="Logout"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" onAction="#onClose" text="Close"/>
        </Menu>
    </MenuBar>
    <AnchorPane prefHeight="369.0" prefWidth="600.0">
        <Button layoutX="30.0" layoutY="265.0" mnemonicParsing="false" onAction="#onApplyForRoomButtonClick"
                prefHeight="54.0" prefWidth="90.0" text="Apply for a room" textAlignment="CENTER" wrapText="true"
                style="-fx-background-color: #87c0f0">
            <font>
                <Font size="9.0"/>
            </font>
        </Button>
        <Button layoutX="180.0" layoutY="265.0" mnemonicParsing="false" onAction="#onWardenDetailsButtonClick"
                prefHeight="54.0" prefWidth="90.0" text="Warden Details" textAlignment="CENTER" wrapText="true"
                style="-fx-background-color: #87c0f0">
            <font>
                <Font size="9.0"/>
            </font>
        </Button>
        <Button layoutX="330.0" layoutY="265.0" mnemonicParsing="false" onAction="#onRoommateDetailsButtonClick"
                prefHeight="54.0" prefWidth="90.0" text="Roommate Details" textAlignment="CENTER" wrapText="true"
                style="-fx-background-color: #87c0f0">
            <font>
                <Font size="9.0"/>
            </font>
        </Button>
        <Button layoutX="470.0" layoutY="265.0" mnemonicParsing="false" onAction="#onVacateRoomButtonClick"
                prefHeight="54.0" prefWidth="90.0" text="Vacate Room" textAlignment="CENTER" wrapText="true"
                style="-fx-background-color: #87c0f0">
            <font>
                <Font size="9.0"/>
            </font>
        </Button>
        <Text fx:id="welcomeDetailsText" layoutX="16.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0"
              wrappingWidth="321.0"/>
        <ImageView fitHeight="60.0" fitWidth="60.0" layoutX="533.0" layoutY="4.0" pickOnBounds="true"
                   preserveRatio="true">
            <Image url="@../../images/bmsce-logo.png"/>
        </ImageView>
        <ImageView fitHeight="172.0" fitWidth="600.0" layoutY="64.0" pickOnBounds="true">
            <Image url="@../../images/hostel.jpg"/>
        </ImageView>
        <Text layoutX="16.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Hello,">
            <font>
                <Font size="24.0"/>
            </font>
        </Text>
    </AnchorPane>
</VBox>
